<body>
  <div class="main-container">
    <h1><span>URL shortener</span></h1>
    <div class="form">
      <div class="field">
        <label></label>
        <div class="f-wrapper">
          <input placeholder="Enter your url here" data-clear="false"  id="txtURL" data-key="url" type="url" class="text-field">
          <p class="validator" data-v-type="url-validator" data-field="#txtURL">
            <span class="error-message">Please provide a valid url to shorten</span>
          </p>
        </div>
      </div>
      <div class="field btns">
        <a class="button default validation-trigger save"><span class="icon-link"></span><span>Shorten it!</span></a>
      </div>
    </div>
  </div>

  <div class="loading-section">
    <div>
      <div>
        <div class="loading-message enter">
          <h1>Url Shortener</h1>
          <p>Loading</p>
        </div>
      </div>
    </div>
  </div>

